      "display_name": "Docker",
      "description": "Container image registry",
      "proxy_supported": true,
      "default_url": "https://registry-1.docker.io/",
      "default_config": {
        "v1Enabled": false,
        "forceBasicAuth": true,
        "httpPort": null,
        "httpsPort": null,
        "subdomain": null
      },
      "api_endpoint": {
        "path": "/v1/repositories/docker/proxy",
        "required_fields": [
          "docker",
          "dockerProxy",
          "httpClient",
          "name",
          "negativeCache",
          "online",
          "proxy",
          "storage"
        ],
        "format_specific_config": {
          "docker": {
            "v1Enabled": false,
            "forceBasicAuth": true,
            "httpPort": null,
            "httpsPort": null,
            "subdomain": null
          },
          "dockerProxy": {
            "indexType": "HUB",
            "cacheForeignLayers": false,
            "foreignLayerUrlWhitelist": []
          }
        }
      }
    },
